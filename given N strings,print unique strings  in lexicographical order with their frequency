#include <iostream>
#include <bits/stdc++.h>
//using maps of c+ STL
using namespace std;

int main() 
{
    int n;
    cin >> n;
    map <string, int> m;
    for(int i=0;i<n;i++)
    {
        string s;
        cin>>s;
        m[s]++;
    }
   for(auto im : m)
   {
       cout << im.first <<" "<< im.second<<endl;
   }
}
